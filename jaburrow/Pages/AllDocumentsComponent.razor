@page "/alldocuments"

<PageTitle>All Documents – Julian Aburrow</PageTitle>

<HeadContent>
    <meta name="description"
          content="Browse all professional documents by Julian Aburrow, including CVs, VCA materials, and STAR-format case studies demonstrating senior-level .NET engineering expertise." />

    <meta property="og:title" content="All Documents – Julian Aburrow" />
    <meta property="og:description"
          content="Access CVs, VCA documents, and STAR-format case studies showcasing senior .NET engineering work by Julian Aburrow." />
</HeadContent>

<MudContainer MaxWidth="MaxWidth.Medium" Class="mt-6">
    <MudCard Elevation="3" Class="p-6 mud-card">
        <MudText Typo="Typo.h3" GutterBottom="true">
            All Documents
        </MudText>

        <MudTabs Elevation="3" Rounded="true" ApplyEffectsToContainer="true" TabPanelsClass="pa-6">

            <MudTabPanel Text="CVs">

                <CVDocumentComponent />

            </MudTabPanel>

            <MudTabPanel Text="VCA">

                <VCADocumentComponent />

            </MudTabPanel>

            <MudTabPanel Text="STAR #1">

                <Star1DocumentComponent />

            </MudTabPanel>

            <MudTabPanel Text="STAR #2">

                <Star2DocumentComponent />

            </MudTabPanel>

            <MudTabPanel Text="STAR #3">

                <Star3DocumentComponent />

            </MudTabPanel>

            <MudTabPanel Text="STAR #4">

                <Star4DocumentComponent />

            </MudTabPanel>

            <MudTabPanel Text="STAR #5">

                <Star5DocumentComponent />

            </MudTabPanel>

        </MudTabs>
    </MudCard>
</MudContainer>

