@page "/github"

<PageTitle>GitHub</PageTitle>

<MudContainer MaxWidth="MaxWidth.Medium" Class="mt-6">

    <MudCard Elevation="3" Class="p-6 mud-card">
        <MudText Typo="Typo.h3" GutterBottom="true">
            GitHub Repos
        </MudText>

        <MudText Typo="Typo.h6">
            Some examples of my coding and various problems that I have solved
        </MudText>

        <br />
        
        <MudTabs Elevation="3" Rounded="true" ApplyEffectsToContainer="true" TabPanelsClass="pa-6">

            <MudTabPanel Text="Manufacturer Manager">

                <ManufacturerManagerComponent />

            </MudTabPanel>

            <MudTabPanel Text="Manufacturer Manager WebAPI">

                <ManufacturerManagerWebAPIComponent />

            </MudTabPanel>

            <MudTabPanel Text="Film Reference">

                <FilmReferenceComponent />

            </MudTabPanel>

            <MudTabPanel Text="Odds And Ends">

                <OddsAndEndsComponent />

            </MudTabPanel>

        </MudTabs>

    </MudCard>

</MudContainer>

